target=arm-linux-gnueabihf

[settings]
os=Linux
arch=armv7
compiler=gcc
compiler.version=11
compiler.libcxx=libstdc++
build_type=Release

[options]

[build_requires]

[env]
CC=$target-gcc
CXX=$target-g++
CHOST=$target
AR=$target-ar
AS=$target-as
RANLIB=$target-ranlib
STRIP=$target-strip
MY_TARGET=ubuntu-native

